,"_id","body","id","name","qualification","gender","email","mobile","username","password","isStaff","saltrounds","bcrypt","hash","save","error","result","send","getallstaff","obj","$regex","find","all","staffs","deletestaff","findOneAndDelete","updatestaff","findOneAndUpdate","addstaff","_","pick"],"sourceRoot":"D:\\GP\\Credit-Hour-System\\controlers\\project\\","sources":["staff-controler.js"],"sourcesContent":["import student from \"../../models/student/student-model\"\r\nimport staff from \"../../models/staff/staff-model\";\r\nimport { validationResult, matchedData } from 'express-validator'\r\nimport multer from \"../../helpers/Multer\"\r\nimport { response } from \"express\";\r\nimport bcrypt from \"bcrypt\"\r\n\r\n// create student\r\nasync function createstaff(req, res, next) {\r\n\r\n    const staf = new staff({\r\n        _id: req.body.id,\r\n        name: req.body.name,\r\n        qualification: req.body.qualification,\r\n        gender: req.body.gender,\r\n        email: req.body.email,\r\n        mobile: req.body.mobile,\r\n        username: req.body.username,\r\n        password: req.body.password,\r\n        isStaff: req.body.isStaff,\r\n    })\r\n    const saltrounds = 10\r\n    staf.password = await bcrypt.hash(staf.password, saltrounds)\r\n    await staf.save((error, result) => {\r\n        if (error) {\r\n            res.send(error)\r\n        }\r\n        else {\r\n            res.send(\"Staff saved\")\r\n        }\r\n    });\r\n}\r\nasync function getallstaff(req, res, next) {\r\n    let obj = {}\r\n    if (\"name\" in req.body) {\r\n        obj.name = { $regex: req.body.name }\r\n    }\r\n    if (\"gender\" in req.body) {\r\n        obj.gender = req.body.gender\r\n    }\r\n    const staffs = await staff.find(obj).all()\r\n    res.send(staffs)\r\n}\r\nasync function deletestaff(req, res, next) {\r\n    const staffs = await staff.findOneAndDelete({\r\n        _id: req.body.id\r\n    })\r\n    res.send(staffs)\r\n}\r\nasync function updatestaff(req, res, next) {\r\n    let obj = {}\r\n    if (\"name\" in req.body) {\r\n        obj.name = req.body.name\r\n    }\r\n    if (\"email\" in req.body) {\r\n        obj.email = req.body.email\r\n    }\r\n    if (\"password\" in req.body) {\r\n        obj.password = req.body.password\r\n    }\r\n\r\n    const staffs = await staff.findOneAndUpdate({\r\n        _id: req.body.id\r\n    }, obj)\r\n    res.send(staffs)\r\n}\r\n\r\n\r\n// async function login(req,res) {\r\n//     let staf = await staff.findOne({_id: req.body._id})\r\n//     let stud = await student.findOne({_id: req.body._id,password: req.body.password})\r\n//     if(staf){\r\n//          res.send(\"Staff found\")\r\n//     }\r\n\r\n//     else if(stud){\r\n//         res.send(\"Student found\")\r\n//     }\r\n//     else{\r\n//         return res.status(404).send('Invalid email or password!')\r\n//     }\r\n\r\n\r\n// }\r\n\r\n\r\nasync function addstaff(req, res, next) {\r\n\r\n    // const {error} = uservalidate(req.body)\r\n    // if(error){\r\n    //     return res.send(error.details.message)\r\n    // }\r\n\r\n    const staf = new staff(_.pick(req.body, ['_id', 'name', 'email', 'password', 'gender'])\r\n        // {\r\n        //     _id: req.body._id,\r\n        //     name: req.body.name,\r\n        //     department: req.body.department,\r\n        //     gender: req.body.gender, \r\n        //     email: req.body.email,\r\n        //     level: req.body.level,\r\n        //     username: req.body.username,\r\n        //     password: req.body.password,\r\n        // }\r\n    )\r\n    const saltrounds = 10\r\n    staf.password = await bcrypt.hash(staf.password, saltrounds)\r\n    await staf.save((error, result) => {\r\n        if (error) {\r\n            res.send(error)\r\n        }\r\n        else {\r\n            res.send(_.pick(staf, ['_id', 'name', 'email']))\r\n        }\r\n    });\r\n}\r\nexport default {\r\n    createstaff,\r\n    getallstaff,\r\n    updatestaff,\r\n    deletestaff,\r\n    addstaff,\r\n\r\n}"],"mappings":";;;;;;AAAA;AACA;AACA;AACA;AACA;AACA;AAA2B;AAAA;AAAA,+CAJ3B;AAAA;AAAA;AAMA;AAAA,SACeA,WAAW;EAAA;AAAA;AAAA;EAAA,0EAA1B,iBAA2BC,GAAG,EAAEC,GAAG,EAAEC,IAAI;IAAA;IAAA;MAAA;QAAA;UAAA;YAE/BC,IAAI,GAAG,IAAIC,sBAAK,CAAC;cACnBC,GAAG,EAAEL,GAAG,CAACM,IAAI,CAACC,EAAE;cAChBC,IAAI,EAAER,GAAG,CAACM,IAAI,CAACE,IAAI;cACnBC,aAAa,EAAET,GAAG,CAACM,IAAI,CAACG,aAAa;cACrCC,MAAM,EAAEV,GAAG,CAACM,IAAI,CAACI,MAAM;cACvBC,KAAK,EAAEX,GAAG,CAACM,IAAI,CAACK,KAAK;cACrBC,MAAM,EAAEZ,GAAG,CAACM,IAAI,CAACM,MAAM;cACvBC,QAAQ,EAAEb,GAAG,CAACM,IAAI,CAACO,QAAQ;cAC3BC,QAAQ,EAAEd,GAAG,CAACM,IAAI,CAACQ,QAAQ;cAC3BC,OAAO,EAAEf,GAAG,CAACM,IAAI,CAACS;YACtB,CAAC,CAAC;YACIC,UAAU,GAAG,EAAE;YAAA;YAAA,OACCC,kBAAM,CAACC,IAAI,CAACf,IAAI,CAACW,QAAQ,EAAEE,UAAU,CAAC;UAAA;YAA5Db,IAAI,CAACW,QAAQ;YAAA;YAAA,OACPX,IAAI,CAACgB,IAAI,CAAC,UAACC,KAAK,EAAEC,MAAM,EAAK;cAC/B,IAAID,KAAK,EAAE;gBACPnB,GAAG,CAACqB,IAAI,CAACF,KAAK,CAAC;cACnB,CAAC,MACI;gBACDnB,GAAG,CAACqB,IAAI,CAAC,aAAa,CAAC;cAC3B;YACJ,CAAC,CAAC;UAAA;UAAA;YAAA;QAAA;MAAA;IAAA;EAAA,CACL;EAAA;AAAA;AAAA,SACcC,WAAW;EAAA;AAAA;AAAA;EAAA,0EAA1B,kBAA2BvB,GAAG,EAAEC,GAAG,EAAEC,IAAI;IAAA;IAAA;MAAA;QAAA;UAAA;YACjCsB,GAAG,GAAG,CAAC,CAAC;YACZ,IAAI,MAAM,IAAIxB,GAAG,CAACM,IAAI,EAAE;cACpBkB,GAAG,CAAChB,IAAI,GAAG;gBAAEiB,MAAM,EAAEzB,GAAG,CAACM,IAAI,CAACE;cAAK,CAAC;YACxC;YACA,IAAI,QAAQ,IAAIR,GAAG,CAACM,IAAI,EAAE;cACtBkB,GAAG,CAACd,MAAM,GAAGV,GAAG,CAACM,IAAI,CAACI,MAAM;YAChC;YAAC;YAAA,OACoBN,sBAAK,CAACsB,IAAI,CAACF,GAAG,CAAC,CAACG,GAAG,EAAE;UAAA;YAApCC,MAAM;YACZ3B,GAAG,CAACqB,IAAI,CAACM,MAAM,CAAC;UAAA;UAAA;YAAA;QAAA;MAAA;IAAA;EAAA,CACnB;EAAA;AAAA;AAAA,SACcC,WAAW;EAAA;AAAA;AAAA;EAAA,0EAA1B,kBAA2B7B,GAAG,EAAEC,GAAG,EAAEC,IAAI;IAAA;IAAA;MAAA;QAAA;UAAA;YAAA;YAAA,OAChBE,sBAAK,CAAC0B,gBAAgB,CAAC;cACxCzB,GAAG,EAAEL,GAAG,CAACM,IAAI,CAACC;YAClB,CAAC,CAAC;UAAA;YAFIqB,MAAM;YAGZ3B,GAAG,CAACqB,IAAI,CAACM,MAAM,CAAC;UAAA;UAAA;YAAA;QAAA;MAAA;IAAA;EAAA,CACnB;EAAA;AAAA;AAAA,SACcG,WAAW;EAAA;AAAA,EAmB1B;AACA;AACA;AACA;AACA;AACA;AAEA;AACA;AACA;AACA;AACA;AACA;AAGA;AAAA;EAAA,0EAlCA,kBAA