req,res,next){\r\n  const projectid=req.body.id\r\n  \r\n  let data={}\r\n  if('name'in req.body){\r\n    data.name=req.body.name\r\n    \r\n  }\r\n  const x=await project.findOneAndUpdate({_id:projectid},data).catch((error)=>{\r\n    res.send(error)\r\n  })\r\n  res.send(x)\r\n} \r\n// delete project\r\nasync function delete_project(req,res,next){\r\n  const projectid=req.body.id\r\n  const proj =await project.findOne({_id:projectid})\r\n  let paths=[]\r\n  paths.push(proj.logo)\r\n  paths.push(proj.background)\r\n  multer.delete_img(paths)\r\n  await project.deleteOne({_id:projectid})\r\n  res.send(\"Done!!!\")\r\n  console.log(\"done\")\r\n}\r\n\r\nexport default {\r\n    createproject,\r\n    find_all_projects,\r\n    update_project,\r\n    delete_project\r\n}"],"mappings":";;;;;;AAAA;AACA;AACA;AAAyC;AAAA;AAAA,+CADzC;AAAA;AAAA;AAEA;AAAA,SACeA,aAAa;EAAA;AAAA;AAgC5B;AACA;AACA;AACA;AAGA;AACA;AAAA;EAAA,4EAvCA,iBAA6BC,GAAG,EAACC,GAAG,EAACC,IAAI;IAAA;IAAA;MAAA;QAAA;UAAA;YACjCC,KAAK,GAAC,IAAAC,kCAAgB,EAACJ,GAAG,CAAC;YAC/BK,OAAO,CAACC,GAAG,CAACN,GAAG,CAACO,IAAI,CAAC;YACrB,IAAGJ,KAAK,CAACK,OAAO,EAAE,EAAC;cACTC,QAAQ,GAAC,IAAAC,6BAAW,EAACV,GAAG,EAAC;gBAAEW,gBAAgB,EAAE;cAAM,CAAC,CAAC;cACrDC,IAAI,GAAC,IAAIC,wBAAO,CAAC;gBACrBC,IAAI,EAACd,GAAG,CAACO,IAAI,CAACO,IAAI;gBAClBC,IAAI,EAACf,GAAG,CAACgB,KAAK,CAACD,IAAI,CAAC,CAAC,CAAC,CAACE,IAAI;gBAC3BC,UAAU,EAAClB,GAAG,CAACgB,KAAK,CAACE,UAAU,CAAC,CAAC,CAAC,CAACD,IAAI;gBACtCE,QAAQ,EAACnB,GAAG,CAACO,IAAI,CAACY;cACrB,CAAC,CAAC;cACFP,IAAI,CAACQ,IAAI,CAAC,UAACC,KAAK,EAAEC,MAAM,EAAK;gBACzB,IAAID,KAAK,EAAE;kBACThB,OAAO,CAACC,GAAG,CAACe,KAAK,CAAC;kBAElBpB,GAAG,CAACsB,IAAI,CAACF,KAAK,CAAC;gBACjB,CAAC,MACI;kBACHhB,OAAO,CAACC,GAAG,CAACgB,MAAM,CAAC;kBACnBrB,GAAG,CAACsB,IAAI,CAACX,IAAI,CAAC;gBAChB;cACF,CAAC,CAAC;YACR,CAAC,MACG;cACAX,GAAG,CAACsB,IAAI,CAACpB,KAAK,CAAC;YACnB;UAAC;UAAA;YAAA;QAAA;MAAA;IAAA;EAAA,CAGJ;EAAA;AAAA;AAAA,SAYcqB,iBAAiB;EAAA;AAAA,EAahC;AAAA;EAAA,gFAbA,kBAAiCxB,GAAG,EAACC,GAAG,EAACC,IAAI;IAAA;IAAA;MAAA;QAAA;UAAA;YACvCuB,CAAC,GAA